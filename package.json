    "vite": "^6.0.11",
    "vite-plugin-svgr": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "name": "react-vite-template",
  "private": true,
  "scripts": {
    "build": "tsc && cross-env NODE_ENV=production vite build && npm run umd",
    "commit": "cz",
    "test": "echo 'test'",
    "dev": "cross-env vite",
    "https": "cross-env HTTPS=true vite",
    "tgz": "npm run build && npm run pack",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --ext ts,tsx ./src --fix",
    "lint:style": "stylelint **/*.{scss,css} --max-warnings 4000",
    "prepare": "husky",
    "preview": "vite preview",
    "db:setup": "./setup-local-mysql.sh",
    "env:setup": "./env-setup.sh",
    "java:dev": "mvn spring-boot:run -Dspring-boot.run.profiles=dev",
    "java:compile": "mvn clean compile",
    "java:prod": "mvn spring-boot:run -Dspring-boot.run.profiles=prod",
    "java:build": "mvn clean package",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "start:dev": "npm run env:setup && npm run java:dev:env",
    "start:prod": "npm run env:setup && npm run java:prod:env",
    "start:docker": "npm run env:setup && docker-compose up",
    "visualizer": "cross-env NODE_ENV=development VITE_USE_VISUALIZER=true npm run build"
  },
  "type": "module",
  "version": "1.0.0"
}
